<div class="center-main-container" align="center">
    <div class="center-container">
        <div class="center-main-container-title">
            <h1><%= l(:assets_list) %></h1>
        </div>
        <div class="inner-container" id="inner_container">
            <%= render :partial => 'assets_list', :locals => { :asset_types => @asset_types, :user => User.current, :open_group => 0, :favourites_asset => @favourites_asset, :favourites_asset_group => @favourites_asset_group } %>
        </div>
    </div>
</div>




<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'assets', :plugin => 'redmine_asset_tracker' %>
    <%= stylesheet_link_tag 'facebox', :plugin => 'redmine_asset_tracker' %>
    <%= stylesheet_link_tag 'datepicker', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'facebox', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'prototype-base-extensions', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'prototype-date-extensions', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'datepicker', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'asset_tracker', :plugin => 'redmine_asset_tracker' %>   
    <%= javascript_include_tag 'toggler', :plugin => 'redmine_asset_tracker' %>  
<% end %>
