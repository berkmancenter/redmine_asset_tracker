<div class="center-main-container" align="center">
    <div class="center-container">
        <div class="center-main-container-title">
            <h1><%= l(:search_result) %></h1>
        </div>
        <div class="inner-container" id="inner_container">
                <%= render :partial => 'search_result' , :locals => { :result_assets => @result_assets, :result_groups => @result_groups, :user => User.current, :open_group => 0 } %>
        </div>
    </div>
</div>

<% content_for :header_tags do %>
    <%= stylesheet_link_tag 'assets', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'prototype-base-extensions', :plugin => 'redmine_asset_tracker' %>
    <%= javascript_include_tag 'asset_tracker', :plugin => 'redmine_asset_tracker' %>
<% end %>